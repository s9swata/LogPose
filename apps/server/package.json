{
  "name": "server",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "bun run --hot src/index.ts",
    "start": "bun run dist/index.mjs"
  },
  "dependencies": {
    "@LogPose/db": "workspace:*",
    "@LogPose/env": "workspace:*",
    "@LogPose/schema": "workspace:*",
    "dotenv": "catalog:",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.8.2",
    "hono-pino": "^0.10.3",
    "pino": "^10.3.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@LogPose/config": "workspace:*",
    "@types/bun": "catalog:",
    "tsdown": "^0.16.5",
    "typescript": "catalog:"
  },
  "trustedDependencies": [
    "supabase"
  ]
}
